{
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "allow-plugins": {
            "composer/installers": true,
            "oomphinc/composer-installers-extender": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "installer-types": ["library", "component"],
        "installer-paths": {
            "../assets/composer-asset/{$vendor}/{$name}/": [
                "type:component"
            ]
        }
    },
    "require": {
        "php": ">=7.2.5",
        "oomphinc/composer-installers-extender": "^2.0",
        "codeigniter/framework": "3.1.13",
        "roave/security-advisories": "dev-master",
        "erusev/parsedown": "^1.7",
        "erusev/parsedown-extra": "^0.8.1",
        "ivantcholakov/codeigniter-phpmailer": "^1.4",
        "guzzlehttp/guzzle": "~7.0",
        "components/jquery": "3.6.0",
        "components/modernizr": "^3.5",
        "components/webfontloader": "^1.6",
        "components/highlightjs": "^9.16"
   }
}
